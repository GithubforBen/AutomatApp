                    <Button mnemonicParsing="false" onAction="#btn_4" prefHeight="220.0" prefWidth="201.0" text="4"
                            GridPane.rowIndex="1"/>
                    <Button mnemonicParsing="false" onAction="#btn_5" prefHeight="201.0" prefWidth="200.0" text="5"
                            GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    <Button mnemonicParsing="false" onAction="#btn_6" prefHeight="212.0" prefWidth="200.0" text="6"
                            GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                    <Button mnemonicParsing="false" onAction="#btn_7" prefHeight="204.0" prefWidth="201.0" text="7"
                            GridPane.rowIndex="2"/>
                    <Button mnemonicParsing="false" onAction="#btn_8" prefHeight="248.0" prefWidth="200.0" text="8"
                            GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                    <Button mnemonicParsing="false" onAction="#btn_9" prefHeight="261.0" prefWidth="200.0" text="9"
                            GridPane.columnIndex="2" GridPane.rowIndex="2"/>
                    <Button mnemonicParsing="false" onAction="#btn_delete" prefHeight="190.0" prefWidth="201.0"
                            text="&lt;-" GridPane.rowIndex="3"/>
                    <Button mnemonicParsing="false" onAction="#btn_0" prefHeight="277.0" prefWidth="200.0" text="0"
                            GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                    <Button mnemonicParsing="false" onAction="#btn_next" prefHeight="215.0" prefWidth="200.0" text="âœ“"
                            textFill="#12da23" GridPane.columnIndex="2" GridPane.rowIndex="3">
                        <font>
                            <Font size="40.0"/>
                        </font>
                    </Button>
                </children>
            </GridPane>
        </children>
    </VBox>
</AnchorPane>
