<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="480.0"
            xmlns="http://javafx.com/javafx/17.0.12" fx:controller="de.schnorrenbergers.automat.controller.LoginController">
    <VBox prefHeight="800.0" prefWidth="480.0">
        <children>
            <Label fx:id="text" prefHeight="53.0" prefWidth="480" text="Password eingeben" textAlignment="CENTER"
                   wrapText="true">
                <font>
                    <Font name="Segoe UI" size="48.0"/>
                </font>
            </Label>
            <GridPane prefHeight="699.0" prefWidth="480.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <children>
                    <Button mnemonicParsing="false" onAction="#btn_1" prefHeight="208.0" prefWidth="201.0" text="1"/>
                    <Button mnemonicParsing="false" onAction="#btn_2" prefHeight="197.0" prefWidth="200.0" text="2"
